<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.slideshow.SlideshowFragment" >

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="382dp"
        android:layout_height="306dp"
        android:padding="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.288">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="80sp"
                android:background="#7C28F1"
                android:text="Date"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="80sp"
                android:background="#7C28F1"
                android:text="Reward"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="80sp"
                android:background="#7C28F1"
                android:text="Cost"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="80sp"
                android:text="02/02/2022"
                android:textAlignment="center"
                android:textColor="#151414" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="120sp"
                android:layout_height="81dp"
                android:src="@drawable/ic_menu_gallery" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="80sp"
                android:text="130 Points"
                android:textAlignment="center"
                android:textColor="#151414" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="100sp"
                android:text="02/03/2022"
                android:textAlignment="center"
                android:textColor="#151414" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="120sp"
                android:layout_height="81dp"
                android:src="@drawable/ic_food_foreground" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="100sp"
                android:text="220 Points"
                android:textAlignment="center"
                android:textColor="#151414" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="100sp"
                android:text="02/05/2022"
                android:textAlignment="center"
                android:textColor="#151414" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="120sp"
                android:layout_height="81dp"
                android:src="@drawable/ic_gift"
                app:srcCompat="@drawable/ic_food_foreground" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="120sp"
                android:layout_height="wrap_content"
                android:width="100sp"
                android:text="140 Points"
                android:textAlignment="center"
                android:textColor="#151414" />

        </TableRow>
    </TableLayout>

    <SearchView
        android:layout_width="74dp"
        android:layout_height="27dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.117" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HISTORY"
        android:textAlignment="center"
        android:textColor="#1B1818"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.052" />

</androidx.constraintlayout.widget.ConstraintLayout>